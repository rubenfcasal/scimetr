
x <- rep(LETTERS[1:3], 3)

factor
function (x = character(), levels, labels = levels, # exclude = NA,
          ordered = is.ordered(x), nmax = NA)

  # y <- unique(x)
  # ind <- sort.list(y)
  # levels <- unique(as.character(y)[ind])
  levels <- unique(x)
  # En el último paso ordenar por orden alfabético
  f <- match(x, levels)

  # Crear factor
  levels(f) <- levels
  class(f) <- c("factor")
  f
