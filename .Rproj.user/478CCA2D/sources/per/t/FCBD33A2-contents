library(openxlsx)

labels <- readWorkbook("Etiquetas de campo 6.xlsx")
labels <- labels[1:62,]
variable.labels <- labels$Biblioteca
names(variable.labels) <- labels$Nombre

library(rwos)
all(names(wosdf) ==  names(variable.labels))
attr(wosdf, "variable.labels") <- variable.labels

as.data.frame(attr(wosdf, "variable.labels"))
save(wosdf, file = "wosdf.RData", compress="xz")